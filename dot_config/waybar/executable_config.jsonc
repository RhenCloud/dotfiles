{
    "layer": "top",
    "height": 30,
    "spacing": 6,
    "icon-size": 20,
    "group/group-power": {
        "orientation": "inherit",
        "drawer": {
            "transition-duration": 500,
            "children-class": "not-power",
            "transition-left-to-right": false
        },
        "modules": ["custom/power", "custom/quit", "custom/lock", "custom/reboot"]
    },
    "group/group-pulseaudio": {
        "orientation": "inherit",
        "drawer": {
            "transition-duration": 500,
            "children-class": "not-pulseaudio",
            "transition-left-to-right": true,
            "click-to-reveal": true
        },
        "modules": ["pulseaudio", "pulseaudio/slider"]
    },
    "group/group-bluetooth": {
        "orientation": "inherit",
        "drawer": {
            "transition-duration": 500,
            "children-class": "not-bluetooth",
            "transition-left-to-right": true,
            "click-to-reveal": true
        },
        "modules": ["custom/bluetooth", "bluetooth"]
    },
    "modules-left": [
        "custom/launcher",
        "dwl/tags",
        "hyprland/workspaces",
        // "cpu",
        // "memory",
        "mpris"
        // "mpd"
    ],
    "modules-center": ["hyprland/window"],
    "modules-right": [
        "tray",
        "privacy",
        "clock",
        "custom/notification",
        "group/group-pulseaudio",
        "network",
        "group/group-power"
    ],
    "bluetooth": {
        "format": "{status}",
        "format-connected": "{num_connections} connected",
        "format-disconnected": "",
        "format-connected-battery": "{device_alias} {device_battery_percentage}%"
    },
    "mpd": {
        "server": "localhost",
        "port": "6600",
        "format": "{stateIcon} {title} - {artist} ({elapsedTime:%M:%S}/{totalTime:%M:%S})",
        "tooltip-format": "{stateIcon} {title} - {artist} ({elapsedTime:%M:%S}/{totalTime:%M:%S})",
        // "title-len": 10,
        "max-length": 50,
        "on-click": "mpc toggle",
        // "on-click": "playerctl play-pause",
        "on-click-right": "playerctl next",
        "on-click-middle": "playerctl previous",
        "state-icons": { "paused": "‚ñ∂", "playing": "ÔÅå" }
    },
    "mpris": {
        "interval": 1,
        "[format]-len": 50,
        "dynamic-len": 50,
        "dynamic-importance-order": ["title", "artist", "position", "length"],
        "dynamic-order": ["title", "artist", "position", "length"],
        // "format": "{player_icon} {title} - {artist} [{position}/{length}]",
        "format": "{player_icon} {dynamic}",
        "format-paused": "{status_icon} <i>{dynamic}</i>",
        "player-icons": {
            "default": "ÔÅå",
            "mpv": "üéµ"
        },
        "status-icons": {
            "paused": "‚ñ∂"
        }
    },
    "hyprland/workspaces": {
        "format": "{name}",
        // "format": "{name} {windows} ",
        "format-window-separator": " ",
        // "window-rewrite-default": "ÔÅô",
        "window-rewrite-default": "",
        "window-rewrite": {
            "class<google-chrome>": "Ôâ®", // Windows whose classes are "google-chrome"
            "class<org.kde.dolphin>": "ÔÑï", // Windows whose classes are "filemanager"
            "title<.*youtube.*>": "ÔÖ™", // Windows whose titles contain "youtube"
            "title<.*bilibili.*>": "", // Windows whose titles contain "bilibili"
            "wezterm": "Óûï", // Windows that contain "foot" in either class or title. For optimization reasons, it will only match against a title if at least one other window explicitly matches against a title.
            "code": "Û∞®û",
            "class<io.github.waylyrics.Waylyrics>": "",
            "class<musicfox>": "ÔÄÅ",
            "class<wechat>": "Ôáó",
            "class<.*fcitxfcit.*>": "",
            "title<.*Fcitx5 Input Window.*>": "",
            "class<zoom>": "ÔÄÇ"
        },

        "icon-size": 14,
        "active-only": false,
        "all-outputs": false,
        // "persistent-workspaces": {
        //     "*": 10
        // },
        "on-scroll-up": "hyprctl dispatch workspace r-1",
        "on-scroll-down": "hyprctl dispatch workspace r+1"
    },
    "pulseaudio/slider": {
        "min": 0,
        "max": 100,
        "orientation": "horizontal"
    },
    "pulseaudio": {
        "tooltip": false,
        "format": "{icon}{volume}% ",
        "format-muted": "ÔÄ¶ ",
        "on-scroll-up": "swayosd-client --output-volume 2",
        "on-scroll-down": "swayosd-client --output-volume -2",
        "on-click-middle": "pavucontrol",
        "format-icons": {
            "default": ["ÔÄ¶ ", "ÔÄß ", "ÔÄ® "]
        }
    },
    "hyprland/window": {
        "format": "{}",
        "rewrite": {
            "(.*) - Google Chrome": "$1",
            "Visual Studio Code - (.*)": "VSCode - $1"
        },
        "separate-outputs": true,
        "max-length": 60,
        "icon": true
    },
    "network": {
        "tooltip": false,
        "format-wifi": "Ôá´  {essid}",
        "format-ethernet": "Óºâ ",
        "tooltip-format-disconnected": "Disconnected",
        "on-click-right": "nm-connection-editor"
    },
    "tray": {
        "icon-size": 20,
        "spacing": 10,
        "reverse-direction": true
    },
    "clock": {
        "interval": 1,
        "tooltip-format": "{:%Y-%m-%d %A %H:%M:%S}",
        "locale": "zh_CN.UTF-8",
        "timezones": ["Asia/Shanghai"],
        "format": "{:%H:%M:%S}",
        "format-alt": "{:%YÂπ¥%mÊúà%dÊó• %A}"
    },
    "cpu": {
        "interval": 15,
        "format": "Ôãõ {}%",
        "max-length": 10,
        "on-click": "kitty btop"
    },
    "memory": {
        "interval": 30,
        "format": "ÔÉâ {}%",
        "max-length": 10
    },
    "privacy": {
        "icon-spacing": 10,
        "icon-size": 20,
        "transition-duration": 250,
        "modules": [
            {
                "type": "screenshare",
                "tooltip": true,
                "tooltip-icon-size": 20
            },
            // {
            //     "type": "audio-out",
            //     "tooltip": true,
            //     "tooltip-icon-size": 24
            // },
            {
                "type": "audio-in",
                // "format": "ÔÑ∞ ",
                "tooltip": true,
                "tooltip-icon-size": 20
            }
        ]
    },
    "custom/notification": {
        "tooltip": false,
        "format": "{icon}",
        "format-icons": {
            "notification": "ÔÇ¢<span foreground='red'><sup>ÔëÑ</sup></span> ",
            "none": "ÔÇ¢ ",
            "dnd-notification": "Ôá∑<span foreground='red'><sup>ÔëÑ</sup></span> ",
            "dnd-none": "Ôá∑ ",
            "inhibited-notification": "ÔÇ¢<span foreground='red'><sup>ÔëÑ</sup></span> ",
            "inhibited-none": " ÔÇ¢ ",
            "dnd-inhibited-notification": "Ôá∑<span foreground='red'><sup>ÔëÑ</sup></span> ",
            "dnd-inhibited-none": " Ôá∑ "
        },
        "return-type": "json",
        "exec-if": "which swaync-client",
        "exec": "swaync-client -swb",
        "on-click": "swaync-client -t -sw",
        "on-click-right": "swaync-client -d -sw",
        "escape": true
    },
    "custom/launcher": {
        // "icon-size": 24,
        "format": "ÔåÉ",
        // "format": "Ôåì",
        "on-click": "rofi -show drun --terminal alacritty",
        "on-click-right": "rofi -show run --terminal alacritty"
    },
    "custom/quit": {
        "format": " Û∞çÉ ",
        "tooltip": false,
        "on-click": "hyprctl dispatch exit"
    },
    "custom/lock": {
        "format": " Û∞çÅ ",
        "tooltip": false,
        "on-click": "hyprlock"
    },
    "custom/reboot": {
        "format": " Û∞úâ ",
        "tooltip": false,
        "on-click": "reboot"
    },
    "custom/power": {
        "format": " ÔÄë ",
        "tooltip": false,
        "on-click": "shutdown now"
    }
}
